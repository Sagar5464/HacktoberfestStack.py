class Stack:
    def __init__(self):
        self.stack = []

    def push(self, val):
        self.stack.append(val)  # ✅ Push to top

    def pop(self):
        if not self.stack:
            return None
        return self.stack.pop()  # ✅ Pop from top

s = Stack()
s.push(1)
s.push(2)
print(s.pop())  # ✅ 2
