class Stack:
    def __init__(self):
        self.stack = []

    def push(self, val):
        self.stack.insert(0, val)  # ❌ Wrong direction (inefficient)
    
    def pop(self):
        if not self.stack:
            return None
        return self.stack.pop(0)  # ❌ Removes from front

s = Stack()
s.push(1)
s.push(2)
print(s.pop())  # Expected 2
